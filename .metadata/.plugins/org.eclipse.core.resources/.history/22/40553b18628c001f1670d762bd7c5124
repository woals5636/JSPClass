	// 전달할 파라미터
	String name = "name";
	String age = "age";
	
	// 리다이렉트할 URL에 파라미터 추가
	String location = "ex10_finish.jsp?param1=" + URLEncoder.encode(name, "UTF-8") 
	                + "&param2=" + URLEncoder.encode(name, "UTF-8");
	response.sendRedirect(location);
