<%@page import="java.net.URLEncoder"%>
<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%
	// 전달할 파라미터
	String param1 = "name";
	String param2 = "age";
	
	// 리다이렉트할 URL에 파라미터 추가
	String location = "ex10_finish.jsp?param1=" + URLEncoder.encode(param1, "UTF-8") 
	                + "&param2=" + URLEncoder.encode(param2, "UTF-8");
	response.sendRedirect(location);
	
	String location = "ex10_finish.jsp";
	response.sendRedirect(location);
%>